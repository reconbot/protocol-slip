{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": false,
    "target": "es2017",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "declaration": true,
    "lib": [
      "es2017",
      "esnext.asynciterable"
    ],
  },
  "include": [
    "lib/*",
    "benchmark/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
